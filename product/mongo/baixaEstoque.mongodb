use('ecomm-order')
db.pedidos.find({cliente: "Joao Paulo"}).toArray()[0].itens.forEach((item) => {
    use('ecomm-product')
    db.prodCateg.updateOne({
        "nomeProduto": item.produto,
        quantidadeEmEstoque: {$gte : item.quantidade}
    },
    {
        $inc: {"quantidadeEmEstoque": -item.quantidade}
    })
})

